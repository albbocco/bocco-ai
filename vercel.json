{
  "version": 2,
  "name": "bocco-ai",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "JWT_SECRET": "@bocco_jwt_secret",
    "MOLLIE_API_KEY": "@bocco_mollie_key",
    "DATABASE_URL": "./data/bocco.db"
  }
}
